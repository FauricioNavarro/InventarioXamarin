<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Inventario.Views.MainPage"
             xmlns:resources="clr-namespace:Inventario.Resx"
             Title="{Binding Title}">
             

    
    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Button Text="{x:Static resources:AppResources.EtiquetaProductos}" Command="{Binding GetProductos}"></Button>
        <Button Text="{x:Static resources:AppResources.EtiquetaCompras}" Command="{Binding GetCompras}"></Button>
        <Button Text="{x:Static resources:AppResources.EtiquetaFacturas}" Command="{Binding GetFacturas}"></Button>
    </StackLayout>

</ContentPage>